	<subDataset name="tableDataset" uuid="4df403d2-e4ec-4751-adb6-b2f6544776e1">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="MySQL"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<queryString language="SQL">
			<![CDATA[SELECT 
    wts.kode_transaction,
    wts.kode_car,
    GROUP_CONCAT(DISTINCT wts.kode_service ORDER BY wts.kode_service SEPARATOR ', ') AS kode_service,
    GROUP_CONCAT(DISTINCT s.name_service ORDER BY s.name_service SEPARATOR ', ') AS name_service,
    GROUP_CONCAT(DISTINCT s.description_service ORDER BY s.description_service SEPARATOR ', ') AS description_service,
    GROUP_CONCAT(DISTINCT s.price ORDER BY s.price SEPARATOR ', ') AS service_price,
    GROUP_CONCAT(DISTINCT wts.working_estimate ORDER BY wts.working_estimate SEPARATOR ', ') AS service_working_estimate,
    GROUP_CONCAT(DISTINCT wts.price_total ORDER BY wts.price_total SEPARATOR ', ') AS service_price_total,
    GROUP_CONCAT(DISTINCT wtp.kode_sparepart ORDER BY wtp.kode_sparepart SEPARATOR ', ') AS kode_sparepart,
    GROUP_CONCAT(DISTINCT sp.nama_sparepart ORDER BY sp.nama_sparepart SEPARATOR ', ') AS nama_sparepart,
    GROUP_CONCAT(DISTINCT sp.price ORDER BY sp.price SEPARATOR ', ') AS sparepart_price,
    GROUP_CONCAT(DISTINCT wtp.working_estimate ORDER BY wtp.working_estimate SEPARATOR ', ') AS sparepart_working_estimate,
    GROUP_CONCAT(DISTINCT wtp.price_total ORDER BY wtp.price_total SEPARATOR ', ') AS sparepart_price_total
FROM 
    working_transaction_service wts
LEFT JOIN 
    service s ON wts.kode_service = s.kode_service
LEFT JOIN 
    working_transaction_sparepart wtp ON wts.kode_transaction = wtp.kode_transaction AND wts.kode_car = wtp.kode_car
LEFT JOIN 
    sparepart sp ON wtp.kode_sparepart = sp.kode_sparepart
GROUP BY 
    wts.kode_transaction, 
    wts.kode_car;]]>
		</queryString>
